#!/bin/sh

mkdir -p cf-release/blobs/rootfs
mv stack-s3/*.tar.gz cf-release/blobs/rootfs/cflinuxfs2.tar.gz
cd cf-release
bundle

cat <<EOF > config/final.yml
---
final_name: cf
min_cli_version: 1.5.0.pre.1001
blobstore:
  provider: local
  options:
    blobstore_path: /tmp/blobs
EOF

bosh -n upload blobs
